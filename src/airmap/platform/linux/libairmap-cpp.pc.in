prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=@CMAKE_INSTALL_FULL_LIBDIR@
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@

Name: libairmap-cpp
Description: AirMap's C++14 client library
Version: @AIRMAPD_VERSION_MAJOR@.@AIRMAPD_VERSION_MINOR@.@AIRMAPD_VERSION_PATCH@
Libs: -L${libdir} -lairmap-cpp
Cflags: -I${includedir}
Requires: openssl
